@Data
@Table(name = "cart")
public class Cart {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long id;
	
	@Column
	private int quantity;
	
	@OneToOne(fetch = FetchType.EAGER)	
	@JoinColumn(name = "userName")
	private UserEntity user;
	
	@ManyToOne(fetch = FetchType.EAGER)
	@JoinColumn(name = "menuId")
	private Menu menu;
	
	
